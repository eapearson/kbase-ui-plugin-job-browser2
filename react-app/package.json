{
    "name": "react-app",
    "version": "0.1.0",
    "private": true,
    "homepage": "./",
    "dependencies": {
        "@craco/craco": "5.5.0",
        "@kbase/ui-lib": "0.2.0-alpha.5",
        "@kbase/ui-components": "0.2.1-alpha.6",
        "@types/bluebird": "*",
        "@types/enzyme": "*",
        "@types/enzyme-adapter-react-16": "*",
        "@types/jest": "*",
        "@types/node": "*",
        "@types/papaparse": "*",
        "@types/react": "*",
        "@types/react-dom": "*",
        "@types/react-redux": "*",
        "@types/react-transition-group": "*",
        "@types/uuid": "*",
        "antd": "3.24.2",
        "bluebird": "3.7.1",
        "craco-antd": "1.14.1",
        "enzyme": "3.10.0",
        "enzyme-adapter-react-16": "1.15.1",
        "es-cookie": "1.3.0",
        "papaparse": "5.1.0",
        "react": "16.10.2",
        "react-dom": "16.10.2",
        "react-redux": "7.1.1",
        "react-scripts": "3.2.0",
        "react-test-renderer": "16.10.2",
        "react-transition-group": "4.3.0",
        "redux": "4.0.4",
        "redux-thunk": "2.3.0",
        "typescript": "3.6.4",
        "uuid": "3.3.3"
    },
    "scripts": {
        "start": "nodemon -w ./craco.config.js -w ./node_modules/@kbase/ui-lib/lib/custom/antd/theme.less --exec 'craco start'",
        "build": "craco build",
        "install-plugin": "bash scripts/install-plugin.bash",
        "testx": "craco test --transformIgnorePatterns  \"<rootDir>[\\/]node_modules[\\/](?!@kbase/ui-lib).+\\.js$\"",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "jest": {
        "collectCoverageFrom": [
            "./src/**/*.{ts,tsx}",
            "!/node_modules/",
            "!src/index.tsx",
            "!src/serviceWorker.ts"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "xproxy": "https://ci.kbase.us",
    "devDependencies": {
        "nodemon": "1.19.4",
        "typedoc": "0.15.0",
        "morgan": "1.9.1",
        "http-proxy-middleware": "0.20.0"
    }
}